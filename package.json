    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "jsx-typescript": "^1.5.0-alpha.5",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "should": "^8.3.0",
    "style-loader": "^0.12.4",
    "stylus": "^0.54.2",
    "stylus-loader": "^0.4.0",
    "todomvc-app-css": "^2.0.6",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-isomorphic-tools": "^2.2.43",
    "webpack-livereload-plugin": "^0.7.0"
